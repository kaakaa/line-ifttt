{
  "name": "LINE ifttt",
  "description": "ifttt request server for LINE BOT API",
  "repository": "https://github.com/kaakaa/line-ifttt",
  "keywords": ["line", "bot", "ifttt", "maker"],
  "env": {
    "LINE_CHANNEL_ID": {
      "description": "Channel ID",
      "required": true
    },
    "LINE_CHANNEL_SECRET": {
      "description": "Channel secret",
      "required": true
    },
    "LINE_CHANNEL_MID": {
      "description": "Channel MID",
      "required": true
    },
    "MAKER_SECRET_KEY": {
      "description": "Secret Key of Maker channel",
      "required": true
    },
    "MAKER_EVENT_NAME_DEFAULT": {
      "description": "Default event name of Maker channel",
      "required": false
    }
  },
  "addons": [
    "fixie:tricycle"
  ]
}
